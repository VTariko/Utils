<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.1"/>
    </startup>
  <appSettings>
    <!-- Предельно допустимое значение процентной нагрузки памяти или ЦПУ -->
    <add key="MaxSafeLoad" value="90"/>
    <!-- Предельно допустимое значение процентной нагрузки на ЦПУ отдельным процессом -->
    <add key="MaxSafeLoadProcess" value="50"/>
  </appSettings>
</configuration>
